{
    "compileOnSave": false,
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false
    },
    "exclude": [
        "node_modules"
    ],
    "filesGlob": [
        "src/**/*.tsx"
    ],
    "files": [
        "src/index.tsx"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
